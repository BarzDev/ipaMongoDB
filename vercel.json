{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@now/node"
    },
    {
      "src": "next.config.js",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/server.js"
    },
    {
      "src": "/.*",
      "dest": "/server.js"
    }
  ]
}
